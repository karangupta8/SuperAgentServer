# SuperAgentServer Environment Variables

# OpenAI API Key (required for ExampleAgent)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom model configuration
MODEL_NAME=gpt-3.5-turbo
TEMPERATURE=0.7


# Server Configuration
HOST=0.0.0.0
PORT=8000
ALLOWED_ORIGINS="http://localhost:3000,https://your-frontend.com"
DEBUG=True


# Logging
LOG_LEVEL=INFO

# Adapter Configuration
MCP_ENABLED=True
WEBHOOK_ENABLED=True
A2A_ENABLED=True
ACP_ENABLED=True

# A2A (Agent-to-Agent) Configuration
# Agent identifier for A2A protocol (auto-generated if not provided)
A2A_AGENT_ID=your_agent_id_here
# Discovery service URL for finding other A2A agents
A2A_DISCOVERY_URL=https://discovery.a2a-protocol.org
# Enable A2A message routing
A2A_MESSAGE_ROUTING=True

# ACP (Agent Communication Protocol) Configuration
# Agent identifier for ACP protocol (auto-generated if not provided)
ACP_AGENT_ID=your_agent_id_here
# ACP message broker URL (for advanced routing)
ACP_BROKER_URL=amqp://localhost:5672
# Enable ACP session persistence
ACP_SESSION_PERSISTENCE=True



# Webhook Configuration
# Base URL for webhook endpoints (used in documentation and external integrations)
WEBHOOK_BASE_URL=https://your-domain.com

# Platform Bot Tokens (for sending responses back to users)
# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Slack Bot Token (get from Slack App settings)
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here

# Discord Bot Token (get from Discord Developer Portal)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

