# SuperAgentServer Environment Variables

# OpenAI API Key (required for ExampleAgent)
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=True

# Logging
LOG_LEVEL=INFO

# Adapter Configuration
MCP_ENABLED=True
WEBHOOK_ENABLED=True

# Webhook Configuration
# Base URL for webhook endpoints (used in documentation and external integrations)
WEBHOOK_BASE_URL=https://your-domain.com

# Platform Bot Tokens (for sending responses back to users)
# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Slack Bot Token (get from Slack App settings)
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here

# Discord Bot Token (get from Discord Developer Portal)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Optional: Custom model configuration
MODEL_NAME=gpt-3.5-turbo
TEMPERATURE=0.7
